{% extends 'base.html.twig' %}

{% block title %} Trouver des joueurs {% endblock %}

{% block body %}
    <h1>Trouve des joueurs proche de toi !</h1>

    {% if app.user.latitude and app.user.longitude %}
        <div id="map"></div>
    {% endif %}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    {{ encore_entry_script_tags('js/locate') }}
{% endblock %}